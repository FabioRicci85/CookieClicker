<Window x:Class="CookieCliker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CookieCliker"
        mc:Ignorable="d"
        Title="Cookie Clicker" Height="450" Width="800">
    <Grid>
        <Rectangle Width="800" Height="450" Stroke="Black" StrokeThickness="1">
            <Rectangle.Fill>
                <ImageBrush ImageSource="Media/qchtergrond.jpg"/>
            </Rectangle.Fill>
        </Rectangle>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
    </Grid>
</Window>